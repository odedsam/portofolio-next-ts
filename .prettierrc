{
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "printWidth": 140,

  "tabWidth": 2,
  "useTabs": false,
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "lf",
  "jsxSingleQuote": false,
  "proseWrap": "always",
  "htmlWhitespaceSensitivity": "strict",
  "embeddedLanguageFormatting": "auto",
  "jsxBracketSameLine": true,
  "bracketSameLine": true
}

